generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model places {
  id       Int     @id @default(autoincrement())
  name     String  @unique
  category String
  rating   String?
}

model public_places {
  id       Int     @id(map: "places_pk") @default(autoincrement())
  name     String  @unique
  category String
  rating   String?

  @@map("public.places")
}
